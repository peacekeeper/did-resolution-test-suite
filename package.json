{
  "name": "danubetech-test-suite",
  "version": "0.0.1",
  "scripts": {
    "cypress:run": "cypress run",
    "cypress:open": "cypress open",
    "combine:reports": "jrm cypress/results/combined-report.xml \"cypress/results/*.xml\"",
    "prereport": "npm run delete:reports",
    "postreport": "npm run combine:reports",
    "clean:reports:old": "rm -R -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports  && mkdir cypress/reports/mocha",
    "clean:reports": "rm -R -f cypress/reports/mocha && mkdir cypress/reports/mocha",
    "pretest": "npm run clean:reports",
    "scripts": "cypress run",
    "combine-reports": "mochawesome-merge cypress/reports/mocha/*.json > cypress/reports/mochareports/report.json",
    "generate-report": "marge cypress/reports/mochareports/report.json -f report_$(date \"+%Y-%m-%d_%H-%M\") -o cypress/reports/mochareports",
    "posttest": "npm run combine-reports && npm run generate-report",
    "slack-message": "npx cypress-slack-reporter --report-dir cypress/reports/mochareports/",
    "slack-message-2": "npx cypress-slack-reporter --report-dir cypress/reports/mochareports --ci-provider custom --custom-url ~/cypress/reports/mochareports/report$(date \"+%Y-%m-%d_%H-%M\").html",
    "test": "npm run scripts || npm run posttest",
    "final": "npx cypress-slack-reporter --report-dir cypress/reports/mochareports --ci-provider custom --custom-url https://danubetech.github.io/did-resolution-test-suite"
  },
  "devDependencies": {
    "cypress": "^9.5.1",
    "cypress-multi-reporters": "^1.5.0",
    "cypress-slack-reporter": "^1.2.1",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "mocha": "^9.2.1",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.1.2",
    "mochawesome-merge": "^4.2.1",
    "mochawesome-report-generator": "^6.1.1",
    "prettier": "2.5.1"
  },
  "dependencies": {
    "mochawesome-slack-reporter": "^1.0.3"
  }
}
